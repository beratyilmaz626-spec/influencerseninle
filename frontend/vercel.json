{
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/videos/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Content-Type": "video/mp4"
      },
      "dest": "/videos/$1"
    },
    {
      "src": "/images/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/images/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
